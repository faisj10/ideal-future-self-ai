{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx: "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLib": false,
    "forRoot": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "noImplicitAny": true
  },
  "include": ["next-env.d.ts"],
  "exclude": ["node_modules"]
}